{
  "name": "hancore",
  "version": "0.0.11",
  "license": "MIT",
  "author": "Haktan Tekin",
  "scripts": {
    "dev": "next dev",
    "build": "rollup -c",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf dist es lib"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/hancore.min.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js",
